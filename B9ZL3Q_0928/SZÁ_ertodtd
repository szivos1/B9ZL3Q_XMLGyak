<?xml version="1.0" encoding="UTF-8"?>

<!ELEMENT A (a, b+, c)>
<!ELEMENT a (#PCDATA)>
<!ATTLIST A a ID #REQUIRED>
<!ELEMENT b (x, y)>
<!ELEMENT x (#PCDATA)>
<!ELEMENT y (#PCDATA)>
<!ELEMENT c (d, e)>
<!ELEMENT d (#PCDATA)>
<!ELEMENT e (#PCDATA)>
<!ELEMENT B (a, b)>
<!ELEMENT a (#PCDATA)>
<!ATTLIST B a ID #REQUIRED>
<!ELEMENT b (x, y)>
<!ELEMENT x (#PCDATA)>
<!ELEMENT y (#PCDATA)>
<!ELEMENT C (a, b)>
<!ELEMENT a (#PCDATA)>
<!ATTLIST A a ID #REQUIRED>
<!ELEMENT b (#PCDATA)>